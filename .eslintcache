e\";\nimport Login from \"./Login\"\nimport {useDispatch} from \"react-redux\";\nimport {auth} from \"./firebase\";\n\nfunction App() {\n    // store.subscribe(() => console.log(\"state Changed !\", store.getState()));\n    const sendMessageIsOpen = useSelector(selectSendMessageIsOpen)\n    const user = useSelector(selectUser);\n    const dispatch = useDispatch();\n\n    useEffect(() => {\n        auth.onAuthStateChanged((user) => {\n            if(user) {\n                // the user is logged in\n                dispatch(login({\n                    displayName: user.displayName,\n                    email: user.email,\n                    photoUrl: user.photoURL,\n                }))\n            }\n        })\n\n    }, [])\n\n    return (\n        <Router>\n            {\n                !user ? (<Login />):\n                    (\n                       <div className={\"app\"}>\n\n                <Header/>\n                <div className=\"app__body\">\n                    <Sidebar />\n                    <Switch  >\n                        <Route path={\"/mail\"}>\n                            <Mail />\n                        </Route>\n                        <Route exact path={\"/\"}>\n                            <EmailList />\n                        </Route>\n                    </Switch>\n               </div>\n                {sendMessageIsOpen && <SendMail />}\n            </div>\n                    )\n\n            }\n\n\n        </Router>\n    );\n}\n\nexport default App;\n","/app/src/serviceWorker.js",[],"/app/src/app/store.js",[],"/app/src/Header.js",[],"/app/src/Sidebar.js",[],"/app/src/Mail.js",[],"/app/src/EmailList.js",[],"/app/src/SendMail.js",["71","72"],"import React, {useEffect, useState} from 'react';\n\nimport './SendMail.css';\nimport CloseIcon from \"@material-ui/icons/Close\";\nimport Button from \"@material-ui/core/Button\";\nimport {useForm} from \"react-hook-form\";\nimport {useDispatch} from \"react-redux\";\nimport {closeSendMessage} from \"./features/mailSlice\";\nimport {db} from \"./firebase\";\nimport firebase from 'firebase';\n\nfunction SendMail() {\n    const {register, handleSubmit, watch, errors} = useForm();\n    const dispatch = useDispatch();\n\n    useEffect(() => console.log(\"rendering sendMail\"));\n    const onSubmit = (formData) => {\n        console.log(formData);\n        const {to,subject,message} = formData;\n        db.collection(\"emails\").add({\n            to: to,\n            subject: subject,\n            message: message,\n            timestamp: firebase.firestore.FieldValue.serverTimestamp()\n        }).then((success)=> {\n            dispatch(closeSendMessage());\n            console.log(success)})\n            .catch((error) => (console.log(error)));\n    }\n\n    return (\n        <div className={\"sendMail\"}>\n            <div className=\"sendMail__header\">\n                <h3>New Message</h3>\n                <div className=\"close__wrap\">\n                <CloseIcon className={\"sendMail__close\"} onClick={() => dispatch(closeSendMessage())}/>\n                </div>\n            </div>\n\n            <form onSubmit={handleSubmit(onSubmit)}>\n                <input name={\"to\"} placeholder={\"To\"} type=\"text\" ref={register({required: true})}/>\n                {errors.to && <p className={\"sendMail__error\"}>To is required</p>}\n                <input name={\"subject\"} placeholder={\"Subject\"} type=\"text\" ref={register({required: true})}/>\n                {errors.subject && <p className={\"sendMail__error\"}>subject is required</p>}\n                <input name={\"message\"} placeholder={\"Message...\"} type=\"text\" className={\"sendMail__message\"} ref={register({required: true})}/>\n                {errors.message && <p className={\"sendMail__error\"}>Message is required</p>}\n                <div className=\"sendMail__options\">\n                    <Button\n                        className={\"sendMail__send\"}\n                        variant={\"contained\"}\n                        color={\"primary\"}\n                        type={\"submit\"}\n                    >Send</Button>\n                </div>\n            </form>\n        </div>\n    )\n};\n\nexport default SendMail;","/app/src/Login.js",[],"/app/src/firebase.js",[],"/app/src/features/mailSlice.js",[],"/app/src/features/userSlice.js",[],"/app/src/SidebarOption.js",[],"/app/src/Section.js",[],"/app/src/EmailRow.js",[],{"ruleId":"73","severity":1,"message":"74","line":5,"column":49,"nodeType":"75","messageId":"76","endLine":5,"endColumn":53},{"ruleId":"73","severity":1,"message":"77","line":12,"column":8,"nodeType":"75","messageId":"76","endLine":12,"endColumn":13},{"ruleId":"78","severity":1,"message":"79","line":35,"column":8,"nodeType":"80","endLine":35,"endColumn":10,"suggestions":"81"},{"ruleId":"73","severity":1,"message":"82","line":1,"column":27,"nodeType":"75","messageId":"76","endLine":1,"endColumn":35},{"ruleId":"73","severity":1,"message":"83","line":13,"column":36,"nodeType":"75","messageId":"76","endLine":13,"endColumn":41},"no-unused-vars","'Link' is defined but never used.","Identifier","unusedVar","'store' is defined but never used.","react-hooks/exhaustive-deps","React Hook useEffect has a missing dependency: 'dispatch'. Either include it or remove the dependency array.","ArrayExpression",["84"],"'useState' is defined but never used.","'watch' is assigned a value but never used.",{"desc":"85","fix":"86"},"Update the dependencies array to be: [dispatch]",{"range":"87","text":"88"},[1202,1204],"[dispatch]"]